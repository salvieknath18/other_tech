   "source": [
    "# Tower of Hanoi\n",
    "\n",
    "def hanoi(n, beg, aux, end):\n",
    "    if n >= 1:\n",
    "        hanoi(n-1, beg, end, aux)\n",
    "        print(f\"{beg} to {end}\")\n",
    "        hanoi(n-1, aux, beg, end)\n",
    "        \n",
    "hanoi(4, \"A\", \"B\", \"C\")"
   ]
